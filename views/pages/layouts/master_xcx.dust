	<script>
            function connect_zhichi(){
            }

		   function init() {
            var current_user ="";
            var arr = document.cookie.split("; ");
            for( var i = 0; i < arr.length; i++){
                var item = arr[i].split("=");
                if(item[0] == "hyb_luntan2"){
                  current_user = item[1];
                  break;
                }
            }


            current_user = decodeURIComponent(current_user);
            var auth = current_user.split('$$$$');
            if(auth && auth.length == 3){
			  $('.buttonTJ').attr('tj_userId',auth[2]);
			}
        };


        if(window.addEventListener){
            window.addEventListener("load",init,false);
        }else{
            window.attachEvent("onload",init);
        }

        ;String.prototype.trim = function(){
            return this.replace(/(^\s+)|(\s+$)/g, "");
        }
        ;function handle(ev){
            var $this = $(ev);
            var val = $this.val().trim(),
                length = val.length;
            $this.siblings('p').text('最多可输入' + (1000 - length) + '个字');
        }
	</script>
</body>
